{
  "routes": [
    { "src": "/marathon-image.jpg", "dest": "/marathon-image.jpg" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "marathon-image.jpg", "use": "@vercel/static" }
  ]
}
