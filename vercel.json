{
  "routes": [
    { "src": "/running.jpg", "dest": "/running.jpg" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "builds": [
    { "src": "running.jpg", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "app.js", "use": "@vercel/static" }
  ]
}
