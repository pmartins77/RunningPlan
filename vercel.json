{
    "routes": [
        {
            "src": "/running.jpg",
            "dest": "/running.jpg"
        },
        {
            "src": "/app.js",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "styles.css",
            "use": "@vercel/static"
        },
        {
            "src": "app.js",
            "use": "@vercel/static"
        },
        {
            "src": "running.jpg",
            "use": "@vercel/static"
        }
    ]
}